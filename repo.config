<Repository>
  <!-- Common settings -->
  <Name>Templates-CoreXT5</Name>
  <Description>CoreXT v5 template for OneBranch</Description>

  <!-- Path is sometimes used by build drop -->
  <Path>Toolsets</Path>
  <Owners>wabuild</Owners>

  <!-- Precheckin validation -->
  <CodeReviewEmailTo>wabuild</CodeReviewEmailTo>
  <DevPackagePublishServer>https://msazure.pkgs.visualstudio.com/_packaging/Dev/nuget/v3/index.json</DevPackagePublishServer>

  <!-- Official build configuration | email aliases must contain @microsoft.com | separate multiple emails by comma -->
  <BuildEmailTo></BuildEmailTo>
  <OfficialPackagePublishServer>https://msazure.pkgs.visualstudio.com/_packaging/Dev/nuget/v3/index.json</OfficialPackagePublishServer>

  <!-- Name of the CloudBuild Queue for branch -->
  <Q>
    <QueueName>CDP_Template-CoreXT5</QueueName>
  </Q>

  <!-- Official defaultBuildSystem options: VSTS, BuildTracker -->
  <!-- See https://onebranch.visualstudio.com/Build/_wiki/wikis/Build.wiki?wikiVersion=GBwikiMaster&pagePath=%2FBuild%20Process%2FOfficial%20Builds for details about the Official Build process -->
  <OfficialBuild defaultBuildSystem="VSTS" />

  <!-- Buddy defaultBuildSystem options: VSTS, BuildTracker, CloudBuild -->
  <!-- See https://onebranch.visualstudio.com/Build/_wiki/wikis/Build.wiki?wikiVersion=GBwikiMaster&pagePath=%2FBuild%20Process%2FBuddy%20Builds for details about the Buddy Build process -->
  <BuddyBuild defaultBuildSystem="VSTS" />

  <!-- Enable localization HB check-in as direct commit -->
  <Localization>
    <CommitType>DirectCommit</CommitType>
    <OverrideCopyBackIncludePatterns>*.lcl</OverrideCopyBackIncludePatterns>
  </Localization>

  <!-- Separate multiple emails by semicolon -->
  <!--<CodeReview>
    <RequiredReviewers>willbar</RequiredReviewers>
    <OptionalReviewers>magitbldsup</OptionalReviewers>
  </CodeReview>-->

  <!-- Trust Services Automation (TSA) for SDL log processing -->
  <!-- Onboard to TSA: https://microsoft.sharepoint.com/teams/WAG/EngSys/Implement/OneBranch/Onboard%20to%20TSA.aspx -->
  <!--<TSA>
    <Notification>TeamDL@microsoft.com,alias@microsoft.com</Notification>
    <AreaPath>BranchName\ProjectName\SubProject</AreaPath>
    <UsePPE>false</UsePPE>
    <ToolsOptOut>APIScan;PoliCheck</ToolsOptOut> -->
    <!-- The branch against which TSA will create alerts [optional: can be a comma\semicolon delimited list of case-sensitive branchnames]. -->
    <!-- <DefaultBranches>master,develop</DefaultBranches>
  </TSA>-->

</Repository>
